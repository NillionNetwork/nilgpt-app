{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@ui/*": [
        "./src/components/ui/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
}